{
  "Description": "A simple Lambda function saying \"Hello\".",
  "AWSTemplateFormatVersion": "2010-09-09",
  "Transform": "AWS::Serverless-2016-10-31",
  "Resources": {
    "HelloWorld": {
      "Type": "AWS::Serverless::Function",
      "Properties": {
        "Handler": "by.vitali.demo.function.pojo.Hello",
        "Runtime": "java8",
        "CodeUri": "s3://vitalibucket2702/demo-1.0.0.jar",
        "Events": {
          "GetResource": {
            "Type": "Api",
            "Properties": {
              "Path": "/input2",
              "Method": "post"
            }
          }
        }
      }
    }
  }
}






